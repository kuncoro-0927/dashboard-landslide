{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "gregorp/vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/_next/static/(.*)",
            "dest": "/public/_next/static/$1"
        },
        {
            "src": "/(.*\\.(css|js|map|ico|svg|png|jpg|jpeg|gif|webp|ttf|woff|woff2|eot))",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "installCommand": "composer install --no-dev && npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "public"
}
