{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },

    "installCommand": "composer dump-autoload --optimize --no-dev && php artisan optimize:clear",
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/(.*\\.(css|js|map|ico|svg|png|jpg|jpeg|gif|webp|ttf|woff|woff2|eot))",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
